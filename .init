export PATH="/Users/marcelofelix/.rbenv/shims:${PATH}"
source /usr/local/Cellar/rbenv/0.4.0/completions/rbenv.bash
#[ [-s ~/.bashrc ]] && source ~/.bashrc
[[ -s ~/dotfiles/.env ]] && source ~/dotfiles/.env
[[ -s ~/dotfiles/.alias ]] && source ~/dotfiles/.alias
cp ~/dotfiles/.vimrc ~/.
cp ~/dotfiles/.tmux.conf ~/.
[[ -s ~/dotfiles/.layout ]] && source ~/dotfiles/.layout
tmux source-file ~/.tmux.conf
source /usr/local/etc/bash_completion.d/git-completion.bash
source /usr/local/etc/bash_completion.d/docker

cd ~/.vim

if [ ! -d "bundle/vim-autoclose" ]; then
    cd ~/.vim/bundle
    git clone https://github.com/Townk/vim-autoclose 
fi

if [ ! -d "bundle/ctrlp.vim" ]; then 
    cd ~/.vim/bundle
    git clone https://github.com/kien/ctrlp.vim
fi

if [ ! -d "bundle/emmet-vim" ]; then
    cd ~/.vim/bundle
    git clone https://github.com/mattn/emmet-vim 
fi

if [ ! -d "bundle/nerdtree" ]; then
    cd ~/.vim/bundle
    git clone https://github.com/scrooloose/nerdtree
fi

if [ ! -d "bundle/vim-easymotion" ]; then
    cd ~/.vim/bundle
    git clone https://github.com/easymotion/vim-easymotion
fi

if [ ! -d "bundle/vim-surround" ]; then
    cd ~/.vim/bundle
    git clone https://github.com/tpope/vim-surround
fi

